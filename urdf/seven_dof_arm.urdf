<?xml version="1.0"?>
<robot name="arm_model">


    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <joint name="bottom_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="base_link"/>
        <child link="bottom_link"/>
    </joint> 

    <link name="bottom_link">
        <visual>
            <geometry>
                <box size="1 1 0.02"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 -0.04"/>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <box size="1 1 0.02"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 -0.04"/>
        </collision>
    </link>

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <origin rpy="1.570795 0 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="shoulder_roll_joint" type="revolute">
        <parent link="base_link"/>
        <child link="shoulder_roll_link"/>
        <origin rpy="0 1.570795 0.0" xyz="0 0 0.05"/>
        <axis xyz="-1 0 0"/>
        <limit effort="300" lower="-2.61799387799" upper="1.98394848567" velocity="1"/>
        <dynamics damping="50" friction="1"/>
    </joint>

    <link name="shoulder_roll_link" >
        <visual>
            <geometry>
                <cylinder length="0.08" radius="0.04"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 0"/>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.08" radius="0.04"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <joint name="shoulder_pitch_joint" type="revolute">
        <parent link="shoulder_roll_link"/>
        <child link="shoulder_pitch_link"/>
        <origin rpy="-1.570795 0 1.570795" xyz="-0.041 0.0021 0.0"/>
        <axis xyz="1 0 0"/>
        <limit effort="300" lower="-1.19962513147" upper="1.89994105047" velocity="1"/>
        <dynamics damping="50" friction="1"/>
    </joint>

    <link name="shoulder_pitch_link">
        <visual>
            <origin rpy="0 1.570795 0" xyz="-0.002 0 0.04"/>
            <geometry>
                <box size="0.14 0.04 0.04"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin rpy="0 1.570795 0" xyz="-0.002 0 0.04"/>
            <geometry>
                <box size="0.14 0.04 0.04"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
        </inertial>
      </link>

      <joint name="elbow_roll_joint" type="revolute">
        <parent link="shoulder_pitch_link"/>
        <child link="elbow_roll_link"/>
        <origin rpy="3.14159 1.570795 0" xyz="-0.002 0 0.1206"/>
        <axis xyz="-1 0 0"/>
        <limit effort="300" lower="-2.61799387799" upper="0.705631162427" velocity="1"/>
        <dynamics damping="50" friction="1"/>
      </joint>

      <link name="elbow_roll_link">
        <visual>
            <geometry>
                <cylinder length="0.06" radius="0.02"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="-0.015 0 -0"/>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.06" radius="0.02"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="-0.015 0 -0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
        </inertial>
      </link>

      <joint name="elbow_pitch_joint" type="revolute">
            <parent link="elbow_roll_link"/>
            <child link="elbow_pitch_link"/>
            <origin rpy="0.055 1.570795 0" xyz="-0.035 0 0"/>
            <axis xyz="1 0 0"/>
            <limit effort="300" lower="-1.5953400194" upper="1.93281579274" velocity="1"/>
            <dynamics damping="50" friction="1"/>  
      </joint>
      
      <link name="elbow_pitch_link">
        <visual>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 -0.12"/>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.22 0.04 0.04"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 -0.12"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
        </inertial>
      </link>

      <joint name="wrist_roll_joint" type="revolute">
        <parent link="elbow_pitch_link"/>
        <child link="wrist_roll_link"/>
        <origin rpy="0 1.570795 3.14159" xyz="0 0.0081 -0.248"/>
        <axis xyz="1 0 0"/>
        <limit effort="300" lower="-2.61799387799" upper="2.6128806087" velocity="1"/>
        <dynamics damping="50" friction="1"/>
      </joint>

      <link name="wrist_roll_link">
        <visual>
            <geometry>
                <cylinder length="0.06" radius="0.02"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 -0.0"/>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.06" radius="0.02"/>
            </geometry>
            <origin rpy="0 1.570795 0" xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>        
        </inertial>
      </link>
</robot>